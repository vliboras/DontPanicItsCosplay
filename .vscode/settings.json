            {
                "name": "Remote Flash JLink",
                "command": "JLink -commanderscript cmake-build-image/firmware/application/flash-remote.jlink"
            }
        ],
        "defaultColor": "white",
        "reloadButton": "â†»",
        "loadNpmCommands": false
    },
