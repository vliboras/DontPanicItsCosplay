{
    "cmake.configureSettings": {
        "CMAKE_TOOLCHAIN_FILE": "cmake/arm-gcc-toolchain.cmake"
    },
    "cmake.buildDirectory": "${workspaceFolder}/cmake-build-image",
    "files.associations": {
        "stdint-gcc.h": "c",
        "stdint.h": "c",
        "cstdint": "c",
        "lpc2292.h": "c",
        "system_clock.h": "c",
        "uart.h": "c"
    },
    "actionButtons": {
        "commands": [
            {
                "name": "Flash JLink",
